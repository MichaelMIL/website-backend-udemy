{
  "name": "Post",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "created_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    },
    "Profile": {
      "type": "belongsTo",
      "model": "Profile",
      "foreignKey": "profileId"
    },
    "Catagory": {
      "type": "belongsTo",
      "model": "Catagory",
      "foreignKey": "catagoryId"
    },
    "PostImage": {
      "type": "hasMany",
      "model": "PostImage"
    },
    "Comments":{
      "type":"hasMany",
      "model":"Comment",
      "foreignKey":""
    }
  },
  "acls": [
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "editor",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
